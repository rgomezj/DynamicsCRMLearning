    <DataSet Name="Students">
      <Query>
        <DataSourceName>DynamicsCRM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>&lt;fetch version="1.0" output-format="xml-platform" mapping="logical" distinct="true"&gt;
  &lt;entity name="contact"&gt;
    &lt;attribute name="contactid" /&gt;
    &lt;attribute name="pert_universalnumber" /&gt;
    &lt;attribute name="fullname" /&gt;
    &lt;attribute name="pert_pereffectivedate" /&gt;
    &lt;attribute name="pert_registrationdate" /&gt;
    &lt;attribute name="pert_pepstartdate" /&gt;
    &lt;attribute name="pert_experiencepath" /&gt;
    &lt;attribute name="address1_name" /&gt;
    &lt;filter type="and"&gt;
      &lt;condition attribute="pert_isstudent" operator="eq" value="1" /&gt;
      &lt;condition attribute="statuscode" operator="eq" value="1" /&gt;
      &lt;condition attribute="pert_perstatus" operator="eq" value="100000002" /&gt;
      &lt;condition attribute="pert_experiencepath" operator="in"&gt;
        &lt;value&gt;100000000&lt;/value&gt;
        &lt;value&gt;100000001&lt;/value&gt;
      &lt;/condition&gt;
      &lt;condition attribute="createdon" operator="on-or-after" value="@FromDate" /&gt;
      &lt;condition attribute="createdon" operator="on-or-before" value="@ToDate" /&gt;
    &lt;/filter&gt;
    &lt;link-entity name="pert_trainingprogram" from="pert_trainingprogramid" to="pert_trainingprogramid" alias="pro" link-type='outer' &gt;
      &lt;attribute name="pert_name" /&gt;
