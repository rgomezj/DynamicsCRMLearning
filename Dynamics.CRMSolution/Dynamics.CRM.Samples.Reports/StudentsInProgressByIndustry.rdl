  <DataSets>
    <DataSet Name="ExperienceReports">
      <Query>
        <DataSourceName>DynamicsCRM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=IIF(IsNothing(Parameters!FromDate.Value), CDate("1900-01-01"), Parameters!FromDate.Value)</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>&lt;fetch version="1.0" output-format="xml-platform" mapping="logical" distinct="true" count="5000"&gt;
  &lt;entity name="contact"&gt;
    &lt;attribute name="contactid" /&gt;
    &lt;filter type="and"&gt;
      &lt;condition attribute="pert_isstudent" operator="eq" value="1" /&gt;
      &lt;condition attribute="pert_perstatus" operator="eq" value="100000002" /&gt;
      &lt;condition attribute="pert_experiencepath" operator="eq" value="100000000" /&gt;
      &lt;condition attribute="createdon" operator="on-or-after" value="@FromDate" /&gt;
      &lt;condition attribute="createdon" operator="on-or-before" value="@ToDate" /&gt;
    &lt;/filter&gt;
    &lt;order attribute="contactid" /&gt;
    &lt;link-entity name="pert_experiencereport" from="pert_studentid" to="contactid" alias="rep"&gt;
      &lt;attribute name="pert_startdate" /&gt;
      &lt;attribute name="pert_employerindustry" /&gt;
    &lt;filter type="and"&gt;
      &lt;condition attribute="statuscode" operator="ne" value="100000007" /&gt;
      &lt;condition attribute="pert_reporttype" operator="in"&gt;
        &lt;value&gt;100000001&lt;/value&gt;
        &lt;value&gt;100000000&lt;/value&gt;
      &lt;/condition&gt;
    &lt;/filter&gt;
      &lt;order attribute="pert_startdate" descending="true" /&gt;
    &lt;/link-entity&gt;
  &lt;/entity&gt;
&lt;/fetch&gt;</CommandText>
      </Query>
      <Fields>
        <Field Name="contactid">
          <DataField>contactid</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="rep_pert_startdate">
          <DataField>rep_pert_startdate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="rep_pert_startdateValue">
          <DataField>rep_pert_startdateValue</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="rep_pert_employerindustry">
          <DataField>rep_pert_employerindustry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="rep_pert_employerindustryValue">
          <DataField>rep_pert_employerindustryValue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PagingCookie">
          <DataField>PagingCookie</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MoreRecords">
          <DataField>MoreRecords</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
      </Fields>
