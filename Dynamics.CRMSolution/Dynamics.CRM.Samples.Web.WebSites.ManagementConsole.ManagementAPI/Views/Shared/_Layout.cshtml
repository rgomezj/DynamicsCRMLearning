<!DOCTYPE html>
<html lang="en">
<head>
    <style>
        #progressbar .ui-progressbar-value {
            background-color: #ccc;
        }
        .modal{
    	width: 100%;
    	position: fixed;
    	margin: 0px auto;
    	top: 0px;
    	left: 0px;
    	bottom: 0px;
    	right: 0px;
    	z-index: 1050;
    }
    .modal_wrapper{
    	display: table;
    	overflow: auto;
    	overflow-y: scroll;
    	height: 100%;
    	-webkit-overflow-scrolling: touch;
    	outline: 0;
    	text-align: center;
    	margin: 0px auto;
    }
    .modal-dialog{
    	margin-top: 0px;
    	display: table-cell;
    	vertical-align: middle;
    	margin: 0px 20px;
    }
    </style>
    <meta charset="utf-8" />
    <title>WAM - Management Console</title>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
    <link href="~/Content/toastr.min.css" rel="stylesheet" />
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Bootstrap Core CSS -->
    <link href="~/Theme/AdminTemplate/bower_components//bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- MetisMenu CSS -->
    <link href="~/Theme/AdminTemplate/bower_components/metisMenu/dist/metisMenu.min.css" rel="stylesheet">

    <!-- DataTables CSS -->
    <link href="~/Theme/AdminTemplate/bower_components/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.css" rel="stylesheet">
    <!-- DataTables Responsive CSS -->
    <link href="~/Theme/AdminTemplate/bower_components/datatables-responsive/css/dataTables.responsive.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="~/Theme/AdminTemplate/bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- Timeline CSS -->
    <link href="~/Theme/AdminTemplate/dist/css/timeline.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="~/Theme/AdminTemplate/dist/css/sb-admin-2.css" rel="stylesheet">
    <!-- Morris Charts CSS -->
    <link href="~/Theme/AdminTemplate/bower_components/morrisjs/morris.css" rel="stylesheet">
    <!-- Custom Fonts -->
    <link href="~/Theme/AdminTemplate/bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->


    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div id="wrapper">
        <!-- Navigation -->
        <div id="Navigation">
            <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="Index">WAM - Management Console</a>
                </div>
                <!-- /.navbar-top-links -->
                <div class="navbar-default sidebar" role="navigation">
                    <div class="sidebar-nav navbar-collapse">
                        <ul class="nav" id="side-menu">
                            <li>
                                <a data-bind='click: goHomePage' href="#"><i class="fa fa-dashboard fa-fw"></i>Home</a>
                            </li>
                            <li>
                                <a data-bind='click: goWaitlists' href="#"><i class="fa fa-table fa-fw"></i>Manage Waitlists</a>
                            </li>
                        </ul>
                    </div>
                    <!-- /.sidebar-collapse -->
                </div>
                <!-- /.navbar-static-side -->
            </nav>
        </div>
        <div id="page-wrapper">
            @RenderBody()
        </div>

        <!-- /#page-wrapper -->
        <!-- jQuery -->
        <script src="~/Theme/AdminTemplate/bower_components/jquery/dist/jquery.min.js"></script>
        <!-- Bootstrap Core JavaScript -->
        <script src="~/Theme/AdminTemplate/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
        <!-- Metis Menu Plugin JavaScript -->
        <script src="~/Theme/AdminTemplate/bower_components/metisMenu/dist/metisMenu.min.js"></script>
        <!-- Morris Charts JavaScript -->
        <script src="~/Theme/AdminTemplate/bower_components/raphael/raphael-min.js"></script>
        <script src="~/Theme/AdminTemplate/bower_components/morrisjs/morris.min.js"></script>
        <script src="~/Theme/AdminTemplate/js/morris-data.js"></script>

        <div id="dialog-message" title="Work in Progress" style="display:none">
            <p id="paragraph-dialog-message">
                <span class="ui-icon ui-icon-circle-check" style="float:left; margin:0 7px 50px 0;"></span>
                Please wait until the operation is completed.....
            </p>
        </div>


        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/toast")
        @Scripts.Render("~/bundles/moment")
        @Scripts.Render("~/bundles/numeral")
        @Scripts.Render("~/bundles/knockout")
        @Scripts.Render("~/Scripts/app/common.js")
        @Scripts.Render("~/Scripts/app/navigation.js")
        @*@Scripts.Render("~/bundles/app")*@
        @RenderSection("Scripts", required: false)

      

      
</body>
</html>
